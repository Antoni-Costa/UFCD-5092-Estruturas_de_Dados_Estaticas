   double lado1, lado2, lado3;
  

   Console.WriteLine("Insira o valor do primeiro lado");
   lado1 = double.Parse(Console.ReadLine());

   Console.WriteLine("Insira o valor do segundo lado");
   lado2 = double.Parse(Console.ReadLine());

   Console.WriteLine("Insira o valor do terceiro lado");
   lado3 = double.Parse(Console.ReadLine());

   if (lado1 == lado2 && lado1 == lado3)
   {
       Console.WriteLine("O triângilo é um Triângulo Equilátero.");
   }
   else if (lado1 == lado2 || lado1 == lado3 || lado2 == lado3)
   {
       Console.WriteLine("O triângulo é um Triângulo Isósceles.");
   }
   else
   { 
       Console.WriteLine("O triângulo é um Triângulo Escaleno");
   }
